meta {
  name: get otp
  type: http
  seq: 2
}

post {
  url: https://www.swiggy.com/dapi/auth/sms-otp
  body: json
  auth: none
}

headers {
  Content-Type: application/json
  User-Agent: {{useragent}}
  Cookie: {{swiggy-cookies}}
}

body:json {
  {
    "otp": {{mobileNumber}},
    "_csrf": {{swiggy-csrf}}
  }
}
